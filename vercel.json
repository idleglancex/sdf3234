{
  "version": 2,
  "framework": null,
  "buildCommand": "",
  "outputDirectory": "",
  "functions": {
    "api/prices.js": {
      "memory": 1024,
      "maxDuration": 30
    }
  },
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        { "key": "Access-Control-Allow-Origin", "value": "*" },
        { "key": "Access-Control-Allow-Methods", "value": "GET, OPTIONS" },
        { "key": "Cache-Control", "value": "s-maxage=30, stale-while-revalidate" }
      ]
    }
  ],
  "rewrites": [
    { "source": "/", "destination": "/api/prices" }
  ]
}
